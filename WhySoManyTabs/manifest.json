{
  "manifest_version": 3,
  "name": "WhySoManyTabs",
  "version": "1.0",
  "description": "A chaotic tab limiter that asks the real question: why do you have so many tabs open?",
  "permissions": ["tabs", "storage", "notifications"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "commands": {
  "toggle-wsmt": {
    "suggested_key": {
      "default": "Ctrl+Shift+Y"
    },
    "description": "Toggle WhySoManyTabs"
  }
}

}
